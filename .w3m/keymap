# ===Reset keybind===
keymap H NULL
keymap C-w NULL
keymap L NULL
keymap H NULL
keymap { NULL
keymap } NULL
keymap b NULL
keymap C-u NULL
keymap C-q NULL
keymap C-e NULL
keymap C-y NULL

# ====Command===
keymap :: COMMAND
keymap :h HELP

# ===Tab nabigation===
keymap t GOTO google.com
keymap T TAB_LINK
keymap C-t NEW_TAB
keymap C-wC-w CLOSE_TAB
keymap TAB NEXT_TAB
keymap ^[[Z PREV_TAB  # ^[[Z = shift+TAB
keymap s NEXT_TAB
keymap a PREV_TAB

# ====Page navigation===
keymap f NEXT_LINK
keymap d PREV_LINK
keymap C-f NEXT_PAGE
keymap C-b PREV_PAGE
keymap b PREV_WORD
keymap C-e UP
keymap C-y DOWN

# ====History navigation===
keymap L NEXT
keymap H PREV
keymap ESC-h SELECT_MENU

# ===Misc===
keymap A EXEC_SHELL "printenv | grep $W3M_URL | vim -"
keymap C CHARSET
keymap ESC-SPC MAIN_MENU
