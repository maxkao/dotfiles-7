# ===Reset keybind===
keymap H NULL
keymap C-w NULL
keymap L NULL
keymap H NULL
keymap { NULL
keymap } NULL

# ====Command===
keymap :: COMMAND
keymap :h HELP

# ===Tab nabigation===
keymap t GOTO google.com
keymap T TAB_LINK
keymap C-t NEW_TAB
keymap C-w CLOSE_TAB
keymap TAB NEXT_TAB
keymap ^[[Z PREV_TAB  # ^[[Z = shift+TAB
keymap s NEXT_TAB
keymap a PREV_TAB

# ====Link navigation===
keymap f NEXT_LINK
keymap d PREV_LINK

# ====History navigation===
keymap L NEXT
keymap H PREV
keymap ESC-h SELECT_MENU

# ===Misc===
keymap A EXEC_SHELL "printenv | grep $W3M_URL | vim -"
keymap C CHARSET
keymap ESC-d MAIN_MENU
